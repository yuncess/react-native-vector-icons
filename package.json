{
  "name": "react-native-vector-icons-yuncess",
  "version": "0.1.9",
  "description": "react-native-vector-icons-yuncess",
  "main": "dist/index.js",
  "bin": {
    "generate-icon": "./bin/generate-icon.js"
  },
  "scripts": {
    "test": "eslint index.js {bin,lib}/*.js",
    "build": "rm -rf {Fonts,ZIcon.js,glyphmaps} && mkdir Fonts glyphmaps && npm run build-zicon",
    "build-web": "rm -rf ./dist && babel ./{,lib}/*.js --out-dir ./dist && cp -R ./glyphmaps ./dist/glyphmaps",
    "build-zicon": "node bin/generate-icon zicon/iconfont.css --prefix=zIcon- --componentName=ZIcon --fontFamily=zIcon --template=templates/separated-icon-set.tpl --glyphmap=glyphmaps/ZIcon.json > ZIcon.js && cp zicon/iconfont.ttf Fonts/ZIcon.ttf"
  },
  "keywords": [
    "react-native",
    "react-component",
    "react-native-component",
    "react",
    "mobile",
    "ios",
    "android",
    "osx",
    "windows",
    "macos",
    "ui",
    "icon",
    "icons",
    "vector",
    "retina",
    "font"
  ],
  "author": {
    "name": "Joel Arvidsson",
    "email": "joel@oblador.se"
  },
  "homepage": "https://github.com/yuncess/react-native-vector-icons-yuncess",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuncess/react-native-vector-icons-yuncess.git"
  },
  "license": "MIT",
  "rnpm": {
    "assets": [
      "Fonts"
    ]
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "prop-types": "^15.5.10",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "evil-icons": "^1.8.0",
    "font-awesome": "^4.6.3",
    "ionicons": "^3.0.0",
    "material-design-icons": "^3.0.1",
    "mdi": "^2.1.19",
    "octicons": "^6.0.1",
    "prettier": "^1.5.3"
  }
}
